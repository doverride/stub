(typeattribute can_read_memory)
(typeattribute not_can_read_memory_or_dev_unconfined_subject)
(typeattributeset not_can_read_memory_or_dev_unconfined_subject
    (not (can_read_memory dev_unconfined_subject)))
(neverallow not_can_read_memory_or_dev_unconfined_subject
    o.dev.memory.t (chr_file (read)))

(typeattribute can_write_memory)
(typeattribute not_can_write_memory_or_dev_unconfined_subject)
(typeattributeset not_can_write_memory_or_dev_unconfined_subject
    (not (can_write_memory dev_unconfined_subject)))
(neverallow not_can_write_memory_or_dev_unconfined_subject
    o.dev.memory.t (chr_file (append write)))

(call fs_associate_devtmpfs (dev_object))

(allow dev_unconfined_subject self (capability (sys_rawio)))

(allow dev_unconfined_subject dev_object all_chr_file_perms_except_execmod)

(macro dev_object ((type ARG1))
    (call object (ARG1))
    (typeattributeset dev_object ARG1))

(macro dev_read_memory ((type ARG1))
    (call fs_search_devtmpfs (ARG1))
    (allow ARG1 self (capability (sys_rawio)))
    (allow ARG1 o.dev.memory.t read_chr_file_perms)
    (typeattributeset can_read_memory ARG1))

(macro dev_write_memory ((type ARG1))
    (call fs_search_devtmpfs (ARG1))
    (allow ARG1 self (capability (sys_rawio)))
    (allow ARG1 o.dev.memory.t write_chr_file_perms)
    (typeattributeset can_write_memory ARG1))

(macro dev_unconfined_subject ((type ARG1))
    (typeattributeset dev_unconfined_subject ARG1))

(context dev_null (s.sys.u o.r o.dev.null.t (systemlow systemlow)))

(sidcontext devnull dev_null)

(filecon "/dev/full" char dev_null)
(filecon "/dev/null" char dev_null)
